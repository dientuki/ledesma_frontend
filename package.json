{
  "name": "ledesma",
  "version": "1.0.0",
  "description": "front",
  "main": "index.js",
  "sideEffects": false,
  "scripts": {
    "build:dev": "better-npm-run build:dev",
    "after:build": "npm run svg",
    "svg": "svgo --config=svgo.yml -f ./images -o ./dist/svg",
    "presvg": "rm -rf dist/svg && mkdir dist/svg",
  },
  "betterScripts": {
    "build:prod": {
      "command": "better-npm-run compile",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:dev": {
      "command": "better-npm-run compile",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    }
  },
  "devDependencies": {
    "autoprefixer": "^9.2.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.4",
    "babel-preset-env": "^1.7.0",
    "better-npm-run": "^0.1.0",
    "browserslist": "^2.4.0",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "debug": "^3.0.1",
    "eslint": "^4.19.1",
    "file-loader": "^2.0.0",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^4.4.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.5.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.6.0",
    "stylelint-scss": "^3.3.2",
    "stylelint-webpack-plugin": "^0.10.5",
    "svgo": "^1.1.1",
    "uglify-es": "^3.3.9",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.2",
    "webpack": "^4.22.0",
    "webpack-assets-manifest": "^3.1.0",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "mysql": "^2.16.0",
    "socket.io": "^2.2.0",
    "vanilla-lazyload": "^8.15.2"
  },
  "babel": {
    "presets": [
      "env",
      {
        "modules": false
      }
    ]
  },
  "browserslist": [
    "last 4 versions"
  ]
}
